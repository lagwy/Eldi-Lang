var int e[2];

method int imprime(int h){
	var int aux, i, val;
	if(h > 1) {
		val = h - 1;
		aux = imprime(val);
		i = h * aux;
		return i;
	}
	else {
		return 1;
	}
}

method void main(){
	var int arr2[2],j, arr[5];
	arr2[1] = 2;
	j = read();

	j = imprime(j);
	print (j);
	j = arr2[1] * 5;
	e[0] = j;
}
